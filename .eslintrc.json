{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "object-curly-spacing": 0,
    "operator-linebreak": 0,
    "indent": 0,
    "react/prop-types": 0,
    "no-unused-vars": 1,
    "require-jsdoc": [
      2,
      {
        "require": {
          "FunctionDeclaration": false,
          "MethodDefinition": false,
          "ClassDeclaration": false
        }
      }
    ],
    "max-len": ["warn", { "code": 120 }]
  }
}
